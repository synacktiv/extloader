{
  "manifest_version": 3,
    "name": "Simple extension",
    "version": "1.0",
    "description": "A minimalistic MV3 Cmahrome extension example.",
    "permissions": [
      "cookies",
      "tabs",
      "storage",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ],
    "key": ""
}